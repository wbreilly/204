u.sd = function(data){
sq.dev = (data - mean(data))^2
n = length(data)
usd = sqrt((sum(sq.dev))/(n - 1))
return(usd)
}
u.sd(temp)
sd(temp)
u.sd(temp2)
sd(temp2)
